     <item n="20">
        <label>Enable Copilot Radar Control</label>
        <binding>
          <command>nasal</command>
          <script>
            setprop("/sim/controls/copilot-mode", 1);
            pilot.start();
          </script>
        </binding>
      </item>
         <item n="21">
        <label>Disable Copilot Radar Control</label>
        <binding>
          <command>nasal</command>
          <script>
            setprop("/sim/controls/copilot-mode", 0);
            pilot.stop();
          </script>
        </binding>
